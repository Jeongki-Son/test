<form action="/home/realUpdate/<%= @post.id %>" method="POST">
  <div class="container">
  <div class="form-group">
    <label for="exampleInputEmail1">Email address</label>
    <input name="title" type="text" value="<%= @post.title %>" class="form-control" id="exampleInputEmail1" placeholder="Email">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input name="content" type="text" class="form-control" value="<%= @post.content %>" id="exampleInputPassword1" placeholder="Password">
  </div>
  <button type="submit" class="btn btn-default">Submit</button>
  </div>
</form>